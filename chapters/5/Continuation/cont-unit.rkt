#lang typed/racket

(require "../types/types.rkt"
         "cont-sig.rkt")

(provide cont@)


(define-unit cont@
  (import)
  (export cont^)

  (: id-cont [-> Cont])
  (define id-cont (位 () (位 (val) (make-final-answer val))))

  (: end-cont [-> Cont])
  (define end-cont
    (位 ()
      (displayln "End of Computation!")
      (id-cont)))

  (: apply-cont [-> Cont ExpVal FinalAnswer])
  (define apply-cont
    (位 (cont val)
      (cont val)))

  )
